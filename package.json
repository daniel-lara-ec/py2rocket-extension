{
  "name": "py2rocket-extension",
  "displayName": "Py2Rocket Extension",
  "description": "Extensi√≥n para compilar y desplegar workflows de Py2Rocket",
  "version": "0.8.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "py2rocket.build",
        "title": "Py2Rocket: Build",
        "icon": "$(tools)"
      },
      {
        "command": "py2rocket.buildAndPush",
        "title": "Py2Rocket: Build and Push",
        "icon": "$(cloud-upload)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "py2rocket.build",
          "when": "resourceLangId == python",
          "group": "navigation@1"
        },
        {
          "command": "py2rocket.buildAndPush",
          "when": "resourceLangId == python",
          "group": "navigation@2"
        }
      ],
      "editor/context": [
        {
          "command": "py2rocket.build",
          "when": "resourceLangId == python",
          "group": "py2rocket@1"
        },
        {
          "command": "py2rocket.buildAndPush",
          "when": "resourceLangId == python",
          "group": "py2rocket@2"
        }
      ]
    },
    "configuration": {
      "title": "Py2Rocket",
      "properties": {
        "py2rocket.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Ruta del ejecutable de Python usado para ejecutar py2rocket (por ejemplo, .venv\\Scripts\\python.exe)"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "eslint": "^8.36.0"
  },
  "dependencies": {}
}
